{
   "apiVersion": "v1",
   "items": [
      {
         "apiVersion": "apps/v1",
         "kind": "Deployment",
         "metadata": {
            "name": "deltafi-app-v2-frontend",
            "namespace": "deltafi-app-v2"
         },
         "spec": {
            "replicas": 2,
            "selector": {
               "matchLabels": {
                  "app": "deltafi-app-v2-frontend"
               }
            },
            "template": {
               "metadata": {
                  "labels": {
                     "app": "deltafi-app-v2-frontend"
                  }
               },
               "spec": {
                  "containers": [
                     {
                        "env": [
                           {
                              "name": "DOCKER_IMAGE_TAG",
                              "value": "prod-748f131-20220610-192041"
                           },
                           {
                              "name": "DEPLOYMENT_MODE",
                              "value": "mainnet-prod"
                           }
                        ],
                        "image": "077918681028.dkr.ecr.us-west-2.amazonaws.com/deltafi-app-v2/frontend:prod-748f131-20220610-192041",
                        "name": "main",
                        "ports": [
                           {
                              "containerPort": 80
                           }
                        ]
                     }
                  ],
                  "nodeSelector": {
                     "kubernetes.io/arch": "arm64"
                  },
                  "tolerations": [
                     {
                        "effect": "NoSchedule",
                        "key": "arch",
                        "operator": "Equal",
                        "value": "arm64"
                     }
                  ]
               }
            }
         }
      },
      {
         "apiVersion": "v1",
         "kind": "Service",
         "metadata": {
            "name": "deltafi-app-v2-frontend",
            "namespace": "deltafi-app-v2"
         },
         "spec": {
            "ports": [
               {
                  "port": 80,
                  "protocol": "TCP",
                  "targetPort": 80
               }
            ],
            "selector": {
               "app": "deltafi-app-v2-frontend"
            }
         }
      },
      {
         "apiVersion": "apps/v1",
         "kind": "Deployment",
         "metadata": {
            "name": "deltafi-app-v2-backend",
            "namespace": "deltafi-app-v2"
         },
         "spec": {
            "replicas": 2,
            "selector": {
               "matchLabels": {
                  "app": "deltafi-app-v2-backend"
               }
            },
            "template": {
               "metadata": {
                  "labels": {
                     "app": "deltafi-app-v2-backend"
                  }
               },
               "spec": {
                  "containers": [
                     {
                        "env": [
                           {
                              "name": "DOCKER_IMAGE_TAG",
                              "value": "prod-748f131-20220610-192041"
                           },
                           {
                              "name": "DEPLOYMENT_MODE",
                              "value": "mainnet-prod"
                           }
                        ],
                        "image": "077918681028.dkr.ecr.us-west-2.amazonaws.com/deltafi-app-v2/backend:prod-748f131-20220610-192041",
                        "name": "main",
                        "ports": [
                           {
                              "containerPort": 4000
                           }
                        ]
                     }
                  ],
                  "nodeSelector": {
                     "kubernetes.io/arch": "arm64"
                  },
                  "tolerations": [
                     {
                        "effect": "NoSchedule",
                        "key": "arch",
                        "operator": "Equal",
                        "value": "arm64"
                     }
                  ]
               }
            }
         }
      },
      {
         "apiVersion": "v1",
         "kind": "Service",
         "metadata": {
            "name": "deltafi-app-v2-backend",
            "namespace": "deltafi-app-v2"
         },
         "spec": {
            "ports": [
               {
                  "port": 4000,
                  "protocol": "TCP",
                  "targetPort": 4000
               }
            ],
            "selector": {
               "app": "deltafi-app-v2-backend"
            }
         }
      },
      {
         "apiVersion": "networking.k8s.io/v1",
         "kind": "Ingress",
         "metadata": {
            "annotations": {
               "cert-manager.io/cluster-issuer": "letsencrypt-prod"
            },
            "name": "deltafi-app-v2",
            "namespace": "deltafi-app-v2"
         },
         "spec": {
            "ingressClassName": "nginx",
            "rules": [
               {
                  "host": "app-v2.k8s.deltafi.trade",
                  "http": {
                     "paths": [
                        {
                           "backend": {
                              "service": {
                                 "name": "deltafi-app-v2-backend",
                                 "port": {
                                    "number": 4000
                                 }
                              }
                           },
                           "path": "/api/",
                           "pathType": "Prefix"
                        },
                        {
                           "backend": {
                              "service": {
                                 "name": "deltafi-app-v2-frontend",
                                 "port": {
                                    "number": 80
                                 }
                              }
                           },
                           "path": "/",
                           "pathType": "Prefix"
                        }
                     ]
                  }
               }
            ],
            "tls": [
               {
                  "hosts": [
                     "app-v2.k8s.deltafi.trade"
                  ],
                  "secretName": "deltafi-app-v2-tls"
               }
            ]
         }
      }
   ],
   "kind": "List"
}
